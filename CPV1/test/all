./computorv1 "4*x^3.1 = 0" // float
./computorv1 "4*x^-12 = 0" // neg
./computorv1 "40*x^0 - 12*x^1 = 4*x^0" // one
./computorv1 "4*x^4 = 0" // too big
./computorv1 "5*x^0 + 4*x^1 - 9.3*x^2 = 1*x^0" // greater
./computorv1 "-2*x^0 + 17*x^1 - 4*x^2 = 13*x^1" // smaller
./computorv1 "1*x^0 + 17*x^1 + 4*x^2 = 13*x^1" // zero
./computorv1 "0.9 * x^2 = 0.2 * x^3 + 0.1 * x^3 - 0.3 * x^3" // pyth pb
./computorv1 "0.1 * X^0 + 0.2 * X^0 = 0.3 * X^0" // pyth pb
./computorv1 "0.9 * x^2 = 0.000000000000000000000000000000001 * x^3" // pyth pb
./computorv1 "1 * X^3 + 1 * X^2  = 1 * X^3" // messy reduced form
./computorv1 "4*x^2 = 4*x^2 + 1*x^1 - 1*x^1" // messy reduced form
./computorv1 "-1*x^1 - -1*x^1 = 2*x^1 +- 1*x^1 -+ 1*x^1" // sign problem
./computorv1 "4*x^2 + 43*x^1 = 4*x^2 + 43*x^1" // spe
./computorv1 "5 * X^0 + 0 * X^1 = 4 * X^0" // spe
./computorv1 "-0*x^0 = 0" // spe