#include "fdf.h"


int		main(int argc, char **argv)
{
	int fd;
	char *line;

	fd = open(argv[1], O_RDONLY);
	while (get_next_line(fd, &line))
	{
		free(line);
		printf("%s\n", line);
	}
	free(line);
	return (0);
}
